<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net">

  <object id="AgendamentoServicos_ProxyCreator" type="Spring.Aop.Framework.AutoProxy.ObjectNameAutoProxyCreator, Spring.Aop">
    <property name="ObjectNames">
      <list>
        <value>GetEmpresa*</value>
      </list>
    </property>
  </object>

  <object type="Spring.Scheduling.Quartz.SchedulerFactoryObject, Spring.Scheduling.Quartz">
    <property name="JobType" value  ="GetEmpresa.ExecuteJobs.JobCacheControler, GetEmpresa.ExecuteJobs" />
    <property name="triggers">
      <list>
        <ref object="CacheControlerUpdateTrigger" />
      </list>
    </property>
  </object>


  <object id="CacheControlerUpdate" type="Spring.Scheduling.Quartz.MethodInvokingJobDetailFactoryObject, Spring.Scheduling.Quartz">
    <property name="TargetObject" ref="JobCacheControler" />
    <property name="TargetMethod" value="UpdateCacheControler" />
  </object>

  <object id="CacheControlerUpdateTrigger" type="Spring.Scheduling.Quartz.CronTriggerObject, Spring.Scheduling.Quartz">
    <property name="JobDetail" ref="CacheControlerUpdate" />
    <property name="CronExpressionString" value="0 0/2 * * * ?" />
  </object>

</objects>