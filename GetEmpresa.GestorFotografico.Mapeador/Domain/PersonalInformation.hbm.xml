<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">

  <class name="MundoDaFoto.Domain.PersonalInformation, MundoDaFoto.Domain" table="PersonalInformation" abstract="false">

    <id name="Id" column="Id" unsaved-value="null">
      <generator class="identity"/>
    </id>

    <property name="ZipCode" column="ZipCode"/>
    <property name="Number" column="Number" />
    <property name="Complement" column="Complement" />
    <property name="Bairro" column="Bairro" />
    <property name="Cpf" column="Cpf" />
    <property name="Cnpj" type="String">
      <column name="Cnpj" length="20" sql-type="varchar" not-null="false"/>
    </property>
    <property name="TypeClient" column="TypeClient" />
    <many-to-one name="City" class="MundoDaFoto.Domain.City, MundoDaFoto.Domain" column="City_IdCity" />
    <many-to-one name="Client" class="MundoDaFoto.Domain.Client, MundoDaFoto.Domain" column="Client_IdClient" />
    
  </class>

</hibernate-mapping>
