<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="GetEmpresa.GestorFotografico.Domain.Portifolio.Portifolio, GetEmpresa.GestorFotografico.Domain" table="Portifolio" lazy="false">

    <id name="Id" column="IdPortifolio" unsaved-value="null">
      <generator class="identity"></generator>
    </id>

    <property name="DataCriacao" column="DataCriacao" update="false" />
    <property name="Descricao" column="Descricao" />
    <many-to-one name="ClienteEnsaio" column="IdCliente" class="GetEmpresa.GestorFotografico.Domina.Cliente.Cliente, GetEmpresa.GestorFotografico.Domain" />

    <bag name="Imagens" cascate="all" lazy="true">
      <key column="IdContratoFotogrtafo" />
      <one-to-many class="GetEmpresa.GestorFotografico.Domain.Portifolio.Imagem, GetEmpresa.GestorFotografico.Domain"></one-to-many>
    </bag>
  </class>
</hibernate-mapping>