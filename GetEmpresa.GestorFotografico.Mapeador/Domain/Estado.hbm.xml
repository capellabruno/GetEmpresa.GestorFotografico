<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">

  <class name="MundoDaFoto.Domain.State, MundoDaFoto.Domain" table="State" abstract="false">

    <id name="Id" column="IdState" unsaved-value="null">
      <generator class="identity"/>
    </id>

    <property name="Name" column="Name" />
    <property name="Acronym" column="Acronym" />
    <many-to-one name="Country" class="MundoDaFoto.Domain.Country, MundoDaFoto.Domain" column="Country_IdCountry" />
    
  </class>

</hibernate-mapping>
