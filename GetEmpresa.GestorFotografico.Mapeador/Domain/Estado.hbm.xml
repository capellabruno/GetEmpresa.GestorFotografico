<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">

  <class name="MundoDaFoto.Dominio.Estado, MundoDaFoto.Dominio" table="Estado" abstract="false">

    <id name="Id" column="IdEstado" unsaved-value="null">
      <generator class="identity"/>
    </id>

    <property name="Nome" column="Nome" />
    <property name="Sigla" column="Sigla" />
    <many-to-one name="Pais" class="MundoDaFoto.Dominio.Pais, MundoDaFoto.Dominio" column="Pais_IdPais" />
    
  </class>

</hibernate-mapping>
