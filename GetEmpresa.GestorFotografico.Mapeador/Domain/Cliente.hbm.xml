<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">

  <class name="MundoDaFoto.Dominio.Client, MundoDaFoto.Dominio" table="Client" abstract="false">

    <id name="Id" column="IdClient" unsaved-value="null">
      <generator class="identity"/>
    </id>

    <property name="Name" column="Name" />
    <property name="Email" column="Email" />
        
  <many-to-one name="Municipio" class="MundoDaFoto.Dominio.City, MundoDaFoto.Dominio" column="City_IdCity" />
  <many-to-one name="Perfil" class="MundoDaFoto.Dominio.TypeClient, MundoDaFoto.Dominio" column="TypeClient_Id" />
  
  </class>

</hibernate-mapping>
